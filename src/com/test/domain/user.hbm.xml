<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 4.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-4.0.dtd">

<hibernate-mapping>
    <class name="com.test.domain.User" table="yazduser">
        <cache usage="read-write"/>
        <id name="id" type="java.lang.Integer">
            <column name="userID" precision="10" scale="0" />
             <generator class="sequence">
            	<param name="sequence"></param>
            </generator>
        </id>
        <property name="docid" type="java.lang.Long">
            <column name="DOCID" />
        </property>
        <property name="authorname" type="string">
            <column name="AUTHORNAME" />
        </property>
         <property name="authorcode" type="string">
            <column name="AUTHORCODE" />
        </property>
        <property name="docdate" type="java.sql.Timestamp">
            <column name="DOCDATE" />
        </property>
        <property name="mark" type="java.lang.Double">
            <column name="MARK" />
        </property>
         <property name="type" type="java.lang.Integer">
            <column name="TYPE" />
        </property>    
        
        <property name="llistId" type="java.lang.Integer">
            <column name="LLISTID" />
        </property> 
        
        <property name="sizeNum" type="java.lang.Integer">
            <column name="SIZENUM" />
        </property> 
        
        <property name="addSubScroe" type="java.lang.Double">
            <column name="ADD_SUB_SCROE" />
        </property>
        
        <property name="sumMark" type="java.lang.Double">
            <column name="SUMMARK" />
        </property>
        
        <property name="scroeType" type="java.lang.Integer">
            <column name="SCROETYPE" />
        </property>
              
         <property name="sourceTitle" type="string">
            <column name="SOURCETITLE" />
        </property> 
        
         <property name="remark" type="string">
            <column name="REMARK" />
        </property>  
        
          
    </class>
</hibernate-mapping>
